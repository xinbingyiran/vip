<Window x:Class="AliHelper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AliHelper"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="功能选择" Height="120" Width="240" DataContext="{Binding RelativeSource={RelativeSource Mode=Self}}">
    <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center" TextBlock.FontSize="20">
        <Button Margin="10" Command="{x:Static local:Commands.Ali}">阿里解析</Button>
        <Button Margin="10" Command="{x:Static local:Commands.TianYi}">天翼解析</Button>
    </StackPanel>
</Window>
